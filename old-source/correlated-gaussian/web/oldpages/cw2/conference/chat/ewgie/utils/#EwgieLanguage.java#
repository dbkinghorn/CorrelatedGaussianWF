// Utility functions

	protected String replace(String s, String v)
	{
		return replaceString(s, "$1", v);
	}

	protected String replace(String s, String v1, String v2)
	{
		String tmpStr = replaceString(s, "$1", v1);
		return replaceString(tmpStr, "$2", v2);
	}

	protected String replace(String s, String v1, String v2, String v3)
	{
		String tmpStr = replaceString(s, "$1", v1);
		String tmpStr2 = replaceString(tmpStr, "$2", v2);
		return replaceString(tmpStr2, "$3", v3);
	}

	private String replaceString(String origStr,
		String oldStr, String newStr)
	{
		int pos = origStr.indexOf(oldStr);
		if (pos == -1)
			return origStr;
		String firstPart = origStr.substring(0, pos);
		if (pos == 0)
			firstPart = "";
		String lastPart = origStr.substring(pos + oldStr.length());
		return firstPart + newStr + replaceString(lastPart,
			oldStr, newStr);
	}
}
